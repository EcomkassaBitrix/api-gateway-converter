{
  "tests": [
    {
      "name": "Missing AuthToken",
      "method": "GET",
      "path": "/?uuid=test-uuid-123",
      "expectedStatus": 400,
      "expectedBody": {
        "error": "AuthToken required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Missing uuid",
      "method": "GET",
      "path": "/?AuthToken=test-token",
      "expectedStatus": 400,
      "expectedBody": {
        "error": "uuid required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Invalid token returns error",
      "method": "GET",
      "path": "/?AuthToken=invalid-token&uuid=12345678&group_code=700",
      "expectedStatus": 401,
      "expectedBody": {
        "Status": "Failed"
      },
      "bodyMatcher": "partial"
    }
  ]
}