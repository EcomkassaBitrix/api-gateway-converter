{
  "tests": [
    {
      "name": "Missing AuthToken",
      "method": "GET",
      "path": "/?uuid=test-uuid-123",
      "expectedStatus": 400,
      "expectedBody": {
        "error": "AuthToken required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Missing uuid",
      "method": "GET",
      "path": "/",
      "headers": {
        "X-Auth-Token": "test-token"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "uuid required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Invalid token returns error",
      "method": "GET",
      "path": "/?uuid=12345678&group_code=700",
      "headers": {
        "X-Auth-Token": "invalid-token"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "Status": "Failed"
      },
      "bodyMatcher": "partial"
    }
  ]
}